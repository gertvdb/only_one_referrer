{
  "name": "gertvdb/only_one_referrer",
  "description": "Provide a referrer to redirect to the canonical url for only one node",
  "type": "drupal-module",
  "license": "GPL-2.0+",
  "minimum-stability": "alpha",
  "repositories": {
    "drupal": {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    }
  },
  "require": {
    "php": ">=8.1"
  },
  "require-dev": {
    "drupal/coder": "^8.3",
    "drupal/core-recommended": "^9.0",
    "roave/security-advisories": "dev-latest"
  },
  "autoload": {
    "psr-4": {
      "Drupal\\only_one_referrer\\": "src/"
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true
  }
}
